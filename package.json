{
  "name": "nomz",
  "version": "1.0.0",
  "description": "cs130 project",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "install": "cd client && npm install",
    "test:server": "jest --testTimeout=10000",
    "test:client": "cd client && npm run test",
    "heroku-postbuild": "npm run docs && cd client && npm install --only=dev && npm install && npm run build",
    "lint": "eslint . --fix",
    "test": "npm run lint && npm run test:server && CI=true npm run test:client",
    "docs": "jsdoc -c ./.jsdoc.conf.json -r ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riverchen99/nomz.git"
  },
  "author": "bears",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/riverchen99/nomz/issues"
  },
  "homepage": "https://github.com/riverchen99/nomz#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^5.7.7",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0"
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.3",
    "nodemon": "^1.19.4",
    "react-test-renderer": "^16.11.0",
    "supertest": "^4.0.2"
  },
  "engines": {
    "node": "8.11.4"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "/client/"
    ],
    "testRegex": "tests/.*\\.(js|jsx)$",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
