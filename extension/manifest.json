{
  "name": "Nomz",
  "version": "1.0",
  "description": "View and provide ratings for UCLA Dining Services menus!",
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "scripts": [ "background.js" ]
  },
  "browser_action": {
  	"default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [ "*://menu.dining.ucla.edu/*" ],
      "js": [ "main.js" ]
    }
  ]
}
